{
  "presets": ["es2015", "stage-0", "react"],
  "plugins": ["transform-decorators-legacy", "add-module-exports"],
  "env": {
    "production": {
      "plugins": [
        "transform-react-remove-prop-types",
        "transform-react-constant-elements",
        "transform-react-inline-elements"
      ]
    },
    "development": {
      "presets": ["react-hmre"]
    }
  }
}